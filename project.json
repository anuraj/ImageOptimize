{
  "version": "1.0.0-*",
  "title": "ImageOptimize",
  "description": "A dotnet CLI tool for image optimization.",
  "authors": [
    "Anuraj P"
  ],
  "packOptions": {
    "projectUrl": "https://github.com/anuraj/ImageOptimize",
    "licenseUrl": "https://github.com/anuraj/ImageOptimize/blob/master/License.md"
  },
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "outputName": "dotnet-imgopt"
  },
  "dependencies": {
    "ImageProcessorCore": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": "dnxcore50"
    }
  }
}